ACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"admin-vue.vue","sourceRoot":"src/views/leads/edit","sourcesContent":["<template>\n  <div class=\"viewDashBoard project-route-index\">\n    <transition\n      name=\"fade\"\n      mode=\"out-in\"\n      @beforeLeave=\"beforeLeave\"\n      @enter=\"enter\"\n      @afterEnter=\"afterEnter\"\n    >\n      <div key=\"1\" v-if=\"loader\" class=\"white-row loader-route\">\n        <div class=\"lds-sub-ripple\">\n          <div></div>\n          <div></div>\n        </div>\n      </div>\n      <div v-else class=\"white-row\">\n        <div class=\"show-project-portal-listing\">\n          <!-- Create Project Form  -->\n          <div class=\"form-area\">\n            <div class=\"col-sm-12\">\n              <div class=\"row\">\n                <div class=\"col-sm-12\">\n                  <div class=\"row top-gen-heading\">\n                    <h1 class=\"global-style-heading\">\n                      Edit Lead\n                    </h1>\n                  </div>\n                </div>\n                <div class=\"col-sm-12\">\n                  <form\n                    class=\"brand-form\"\n                    v-on:submit.prevent=\"onUpdate($event)\"\n                  >\n                    <div class=\"row\">\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"title\"\n                              >Title: <span>*</span></label\n                            >\n                            <input\n                              type=\"text\"\n                              class=\"form-control\"\n                              placeholder=\"Title\"\n                              v-model=\"formData.title\"\n                              :class=\"{\n                                'input-border-errors':\n                                  $v.formData.title.$dirty &&\n                                  !$v.formData.title.required\n                              }\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"email\"\n                              >Email: <span>*</span></label\n                            >\n                            <input\n                              type=\"text\"\n                              class=\"form-control\"\n                              placeholder=\"Email\"\n                              v-model=\"formData.email\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"contact\" class=\"control-label\"\n                              >Contact</label\n                            >\n                            <input\n                              type=\"text\"\n                              class=\"form-control\"\n                              placeholder=\"Contact\"\n                              v-model=\"formData.contact\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"zipcode\" class=\"control-label\"\n                              >Zip Code</label\n                            >\n                            <input\n                              type=\"text\"\n                              class=\"form-control\"\n                              placeholder=\"Zip Code\"\n                              v-model=\"formData.zipcode\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"package\" class=\"control-label\"\n                              >Package</label\n                            >\n                            <input\n                              type=\"text\"\n                              class=\"form-control\"\n                              placeholder=\"Package\"\n                              v-model=\"formData.package\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"amount\" class=\"control-label\"\n                              >Amount</label\n                            >\n                            <input\n                              type=\"text\"\n                              class=\"form-control\"\n                              placeholder=\"Amount\"\n                              v-model=\"formData.amount\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"lead_status_id\" class=\"control-label\"\n                              >Lead Status</label\n                            >\n                            <Select2\n                              :options=\"lead_status_options\"\n                              :settings=\"{\n                                placeholder: 'Lead Status',\n                                allowClear: true\n                              }\"\n                              v-model=\"formData.lead_status_id\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"lead_step_id\" class=\"control-label\"\n                              >Lead Steps</label\n                            >\n                            <Select2\n                              :options=\"lead_step_options\"\n                              :settings=\"{\n                                placeholder: 'Lead Steps',\n                                allowClear: true\n                              }\"\n                              v-model=\"formData.lead_step_id\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-12\">\n                        <h5>Tags</h5>\n                      </div>\n                       <div class=\"col-sm-12\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"brand_id\" class=\"control-label\"\n                              >Brand</label\n                            >\n                            <Select2\n                              :options=\"brand_options\"\n                              @change=\"getAmOfBrand\"\n                              :settings=\"{\n                                placeholder: 'Select Brand',\n                                allowClear: true\n                              }\"\n                              v-model=\"formData.brand_id\"\n                              :class=\"{\n                                'input-border-errors':\n                                  $v.formData.brand_id.$dirty &&\n                                  !$v.formData.brand_id.required\n                              }\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"lead_source_id\" class=\"control-label\"\n                              >Lead Source</label\n                            >\n                            <Select2\n                              :options=\"lead_source_options\"\n                              :settings=\"{\n                                placeholder: 'Lead Source',\n                                multiple: true\n                              }\"\n                              v-model=\"formData.lead_source_id\"\n                              :class=\"{\n                                'input-border-errors':\n                                  $v.formData.lead_source_id.$dirty &&\n                                  !$v.formData.lead_source_id.required\n                              }\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-6\">\n                        <div class=\"row\">\n                          <div class=\"form-group\">\n                            <label for=\"am_id\" class=\"control-label\"\n                              >Sales Person</label\n                            >\n                            <Select2\n                              :options=\"ams_options\"\n                              :settings=\"{\n                                placeholder: 'Sales Person',\n                                multiple: true\n                              }\"\n                              v-model=\"formData.am_id\"\n                              :class=\"{\n                                'input-border-errors':\n                                  $v.formData.am_id.$dirty &&\n                                  !$v.formData.am_id.required\n                              }\"\n                            />\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"col-sm-12\">\n                        <div class=\"row\">\n                          <div class=\"form-actions\">\n                            <button\n                              :disabled=\"disableSubmitBtn\"\n                              type=\"submit\"\n                              id=\"save-form\"\n                              class=\"btn btn-primary default-global-btn\"\n                            >\n                              Update\n                            </button>\n                            <div\n                              v-if=\"disableSubmitBtn\"\n                              class=\"form_submit_loader\"\n                            >\n                              <div class=\"lds-ellipsis\">\n                                <div></div>\n                                <div></div>\n                                <div></div>\n                                <div></div>\n                              </div>\n                            </div>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </form>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </transition>\n  </div>\n</template>\n\n<script>\nimport Select2 from \"v-select2-component\";\nimport { validationMixin } from \"vuelidate\";\nconst {\n  required,\n  minLength,\n  email,\n  numeric,\n  url\n} = require(\"vuelidate/lib/validators\");\n\nexport default {\n  mixins: [validationMixin],\n  components: {\n    Select2\n  },\n  computed: {\n    getActiveUserData() {\n      return this.$store.state.auth;\n    }\n  },\n  validations: {\n    formData: {\n      title: {\n        required\n      },\n      brand_id: {\n        required\n      },\n      lead_source_id: {\n        required\n      },\n      am_id: {\n        required\n      },\n      amount: {\n        required\n      },\n\n    }\n  },\n  data() {\n    return {\n      loader: true,\n      brand_options: null,\n      lead_source_options: null,\n      lead_step_options : null,\n      lead_status_options: null,\n      ams_options : null,\n      formData: {\n        id: null,\n        title: null,\n        email: null,\n        contact: null,\n        zipcode: null,\n        package : null,\n        brand_id : null,\n        lead_source_id : null,\n        lead_status_id : null,\n        lead_step_id : null,\n        am_id : null,\n        amount : 0,\n      },\n      disableSubmitBtn: false\n    };\n  },\n  mounted() {\n    document.title = this.$router.app._route.meta.pageTitle;\n    this.editLead();\n    this.getemployees();\n    this.getBrands();\n    this.getLeadSource();\n    this.getLeadStatus();\n    this.getLeadStep();\n  },\n  methods: {\n    async editLead() {\n      const $this = this;\n      const data = $this.$store.dispatch(\"getHttp\", {\n        url: \"/leads/edit/\"+this.$route.params.lead_id\n      });\n      await data.then(function(response) {\n        if (typeof response !== \"undefined\") {\n            $this.formData.id = response.data.data.id;\n            $this.formData.title = response.data.data.title;\n            $this.formData.email = response.data.data.email;\n            $this.formData.contact = response.data.data.contact;\n            $this.formData.zipcode = response.data.data.zip_code;\n            $this.formData.package  = response.data.data.package;\n            $this.formData.brand_id  = response.data.data.brand_id;\n            $this.formData.lead_source_id  = response.data.data.lead_source;\n            $this.formData.lead_status_id  = response.data.data.status_id;\n            $this.formData.lead_step_id  = response.data.data.step_id;\n            $this.formData.am_id  = response.data.data.account_manager;\n            $this.formData.amount = response.data.data.amount;\n            $this.getProjectAms($this.formData.brand_id);\n            $this.loader = false;\n        }\n      });\n    },\n    async getLeadStep(brandid) {\n      const $this = this;\n      const data = $this.$store.dispatch(\"getHttp\", {\n        url: \"/filters/get-lead-step\"\n      });\n      await data.then(function(response) {\n        if (typeof response !== \"undefined\") {\n          $this.lead_step_options = response.data.data;\n        }\n      });\n    },\n    async getLeadStatus(brandid) {\n      const $this = this;\n      const data = $this.$store.dispatch(\"getHttp\", {\n        url: \"/filters/get-lead-status\"\n      });\n      await data.then(function(response) {\n        if (typeof response !== \"undefined\") {\n          $this.lead_status_options = response.data.data;\n        }\n      });\n    },\n    getAmOfBrand() {\n      this.$store.dispatch(\"generalLoader\", {\n        response: true\n      });\n      const $this = this;\n      $this.formData.am_id = null;\n      if ($this.formData.brand_id) {\n        $this.getProjectAms($this.formData.brand_id);\n      } else {\n        $this.ams_options = null;\n        $this.$store.dispatch(\"generalLoader\", {\n          response: false\n        });\n      }\n    },\n    async getProjectAms(brandid) {\n      const $this = this;\n      const data = $this.$store.dispatch(\"getHttp\", {\n        url: \"/filters/get-employees?brand_id=\" + brandid\n      });\n      await data.then(function(response) {\n        if (typeof response !== \"undefined\") {\n          $this.ams_options = response.data.data;\n          $this.$store.dispatch(\"generalLoader\", {\n            response: false\n          });\n        }\n      });\n    },\n    async getLeadSource() {\n      const $this = this;\n      const data = $this.$store.dispatch(\"getHttp\", {\n        url: \"filters/get-lead-source\"\n      });\n      await data.then(function(response) {\n        if (typeof response !== \"undefined\") {\n          $this.lead_source_options = response.data.data;\n        }\n      });\n    },\n    async getBrands() {\n      const $this = this;\n      const data = $this.$store.dispatch(\"getHttp\", {\n        url: \"filters/get-project-brand\"\n      });\n      await data.then(function(response) {\n        if (typeof response !== \"undefined\") {\n          $this.brand_options = response.data.data;\n        }\n      });\n    },\n    async getemployees() {\n      const $this = this;\n      await window.axios\n        .get(\"/filters/get-employees\")\n        .then(resps => {\n          $this.am_options = resps.data.data;\n        })\n        .catch(function(error) {\n          $this.$store.dispatch(\"displayMessageErrors\", {\n            response: error.response,\n            formEvent: null\n          });\n          $this.loader = false;\n        });\n    },\n    async onUpdate(event) {\n      const $this = this;\n      $this.disableSubmitBtn = true;\n      $this.$v.$touch();\n\n      if ($this.$v.$invalid) {\n        $this.$store.dispatch(\"showtostrMessageNotifications\", {\n          message: \"Please fill all the fields to proceed form!\",\n          type: \"error\"\n        });\n        $this.disableSubmitBtn = false;\n        return;\n      }\n\n      const data = $this.$store.dispatch(\"postHttp\", {\n        url: \"leads/lead-update\",\n        formDetails: this.formData,\n        formEvent: event.target\n      });\n      await data.then(function(response) {\n        if (typeof response !== \"undefined\") {\n          $this.$store.dispatch(\"displayMessageErrors\", {\n            response: response,\n            formEvent: event.target\n          });\n          $this.disableSubmitBtn = false;\n          if (Object.keys($this.$route.query).length) {\n              // console.log($this.$route.query);\n              // $this.$router.push(\"/leads?current_page=\"+$this.$route.query.current_page+'&highlited='+$this.$route.params.lead_id);\n              $this.$router.push(\"/leads?current_page=\" + $this.$route.query.current_page +\n                '&search='+$this.$route.query.search +\n                '&emp_id='+$this.$route.query.emp_id +\n                '&brand_id='+$this.$route.query.brand_id +\n                '&lead_status_id='+$this.$route.query.lead_status_id +\n                '&lead_step_id='+$this.$route.query.lead_step_id +\n                '&lead_source_id='+$this.$route.query.lead_source_id +\n                '&lead_source_id_remove_tag='+$this.$route.query.lead_source_id_remove_tag +\n                '&from_date='+$this.$route.query.from_date +\n                '&number_per_page='+$this.$route.query.number_per_page +\n                '&to_date='+$this.$route.query.to_date +\n                '&highlited='+$this.$route.params.lead_id\n              );\n          }\n        }\n      });\n      $this.disableSubmitBtn = false;\n    },\n    beforeLeave(element) {\n      this.prevHeight = getComputedStyle(element).height;\n    },\n    enter(element) {\n      const { height } = getComputedStyle(element);\n\n      element.style.height = this.prevHeight;\n\n      setTimeout(() => {\n        element.style.height = height;\n      });\n    },\n    afterEnter(element) {\n      element.style.height = \"auto\";\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\nbody .form-area .form-group label span {\n  color: red;\n}\nbody .white-row.loader-route {\n  top: 0;\n}\n.fade-enter-active,\n.fade-leave-active {\n  transition-duration: 0.3s;\n  transition-property: opacity;\n  transition-property: height, opacity;\n  transition-timing-function: ease;\n  overflow: hidden;\n}\n.project-route-index {\n  .show-project-portal-listing {\n    .general-filters {\n      .form-group {\n        width: 100%;\n      }\n      .form-actions {\n        width: 100%;\n        button {\n          width: 100%;\n          margin-left: 0;\n          padding: 5px 0;\n        }\n      }\n      span.select2.select2-container.select2-container--default {\n        width: 100% !important;\n      }\n    }\n    h4 {\n      letter-spacing: 0.28px;\n      color: #323f54;\n      font-size: 14px;\n      font-weight: bold;\n      margin-bottom: 0;\n      padding-bottom: 7px;\n    }\n    p {\n      margin: 0;\n      letter-spacing: 0.28px;\n      color: #2a1c0b;\n    }\n    section {\n      border-bottom: 1px solid #ebebeb;\n      padding: 30px 0;\n      &.client-projects {\n        padding-bottom: 0px;\n        h4 {\n          padding: 5px 0 25px;\n        }\n        .wrap-box {\n          background: #faf9f7 0% 0% no-repeat padding-box;\n          border-radius: 3px;\n          padding: 20px 15px;\n          min-height: 231px;\n          margin-bottom: 30px;\n          .box {\n            position: relative;\n            .project-details {\n              p {\n                &.project-id {\n                  background: #ece9df 0% 0% no-repeat padding-box;\n                  border-radius: 3px;\n                  display: inline-block;\n                  letter-spacing: 0.24px;\n                  color: #323f54;\n                  font-size: 12px;\n                  padding: 4px 10px;\n                }\n                &.project-status {\n                  letter-spacing: 0.24px;\n                  color: #323f54;\n                  font-size: 12px;\n                  padding: 3px 0 15px;\n                  font-weight: 400;\n                }\n              }\n              .project-listing {\n                padding: 15px 0 15px;\n                p {\n                  margin: 0;\n                  letter-spacing: 0.28px;\n                  color: #323f54;\n                  font-size: 14px;\n                  font-weight: bold;\n                  text-transform: inherit;\n                  line-height: 25px;\n                }\n              }\n            }\n            .footer-box {\n              display: block;\n              position: relative;\n              width: 100%;\n              padding: 20px 0 10px;\n              border-top: 1px solid rgba(0, 0, 0, 0.1);\n              .usr-role {\n                p {\n                  letter-spacing: 0.24px;\n                  color: #918a82;\n                  font-weight: 300;\n                  font-size: 12px;\n                }\n              }\n            }\n          }\n        }\n      }\n    }\n  }\n}\n</style>"]}]}